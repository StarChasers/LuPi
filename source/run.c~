

void SetGpioFunction(unsigned int gpioRegister, unsigned int function);
void SetGpio(unsigned int gpioRegister, unsigned int value);
void Wait(unsigned int delayInMicroSeconds);

void run(void)
{
	/*
	* Use our new SetGpioFunction function to set the function of GPIO port 16 (OK 
	* LED) to 001 (binary)
	*/
	SetGpioFunction(1,16);
	while(1)
	{
		SetGpio(16, 0);
		Wait(500000);
		SetGpio(16, 1);
		Wait(500000);
	}
}
